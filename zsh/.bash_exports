ZSH_THEME="agnoster"
prompt_context(){}

listening() {
    if [ $# -eq 0 ]; then
        sudo lsof -iTCP -sTCP:LISTEN -n -P
    elif [ $# -eq 1 ]; then
        sudo lsof -iTCP -sTCP:LISTEN -n -P | grep -i --color $1
    else
        echo "Usage: listening [pattern]"
    fi
}

killpid() {
  if [ $# -eq 1 ]; then
    kill -9 $1
  else
    kill
  fi
}

